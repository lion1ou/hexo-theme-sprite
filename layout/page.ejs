<article class="post page" id="page">
  <header class="post-header text-center">

  </header>
  <div class="post-content">
    <% if (page.path === "tags/index.html"){ %>
          <% if (site.tags.length){ %>
                <%- list_tags() %>
          <% } %>
      <% } else if (page.path === "categories/index.html") { %>
        <%- list_categories(site.categories) %>
      <% } else if (page.path === "search/index.html") { %>
        <%- partial('partials/search') %>
      <% } else { %>
        <%-page.content%>
      <% } %>
  </div>
</article>